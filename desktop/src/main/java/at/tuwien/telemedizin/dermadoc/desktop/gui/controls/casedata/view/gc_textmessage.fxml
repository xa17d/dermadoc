<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="400" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="40.0" prefHeight="150.0" vgrow="ALWAYS" />
        <RowConstraints maxHeight="-Infinity" vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Label fx:id="lbMessage" wrapText="true" prefHeight="200.0" prefWidth="200.0" maxWidth="400">
            <GridPane.margin>
                <Insets bottom="15.0" left="15.0" right="25.0" top="15.0" />
            </GridPane.margin>
        </Label>
    </children>
</fx:root>
