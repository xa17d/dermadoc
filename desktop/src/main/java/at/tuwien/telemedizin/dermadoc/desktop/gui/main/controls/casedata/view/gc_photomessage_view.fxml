<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>

<fx:root type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="vbCaseData" minWidth="100.0" prefWidth="400.0" maxWidth="400.0">
            <children>
                <TitledPane fx:id="tpPhotos" text="Photo" minWidth="90.0" prefWidth="350.0" maxWidth="350.0">
                    <content>
                        <AnchorPane>
                            <children>
                                <ImageView fx:id="ivPhoto" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" onMouseClicked="#openPhoto"/>
                            </children>
                        </AnchorPane>
                    </content>
                </TitledPane>
            </children>
            <HBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </HBox.margin>
        </VBox>
    </children>
</fx:root>
